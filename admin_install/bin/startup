#!/bin/sh

cd /srv/core
rm */*.state */*/*.launch_ctrl */*/*.state
/bin/bash /srv/core/bin/admin start

# special case for the "ep1.std01.kaetemi.com" machine - start the admin tool graph sync script
if [ $(hostname) = "ep1.std01.kaetemi.com" ]
	then
	nohup /bin/sh /srv/core/bin/sync_rrd_graphs.sh &
fi
